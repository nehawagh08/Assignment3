    "#Game of \"Functions\"\n",
    "'''Sample List : (8, 2, 3, 0, 7)\n",
    "Expected Output : 20'''\n",
    "\n",
    "def sum1(l1):\n",
    "    sum1=0\n",
    "    for i in l1:\n",
    "        sum1=sum1+i\n",
    "    return sum1\n",
    "l1=list(map(int,input().split()))\n",
    "print(sum1(l1))"
